<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="entity.Student">
    <!--    id 和返回类型，就是封装到什么地方-->
    <select id="getStudent" resultType="entity.Student">
        select * from student
